{
  "name": "maximiwrk/laravel-breadcrumbs",
  "description": "Breadcrumbs for Laravel 11â€“12 (fork of Diglactic/Laravel-Breadcrumbs)",
  "keywords": ["laravel", "breadcrumbs"],
  "homepage": "https://github.com/MaximiWrk/laravel-breadcrumbs",
  "license": "MIT",

  "authors": [
    {
      "name": "MaximiWrk",
      "email": "your@email.com",
      "role": "Maintainer"
    },
    {
      "name": "Sheng Slogar",
      "email": "sheng@diglactic.com",
      "role": "Upstream Maintainer"
    },
    {
      "name": "Dave James Miller",
      "email": "dave@davejamesmiller.com",
      "role": "Original Creator"
    }
  ],

  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "laravel/framework": "^11.0|^12.0",
    "facade/ignition-contracts": "^1.0"
  },

  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^11.0",
    "spatie/phpunit-snapshot-assertions": "^5.1"
  },

  "autoload": {
    "psr-4": {
      "Diglactic\\Breadcrumbs\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Diglactic\\Breadcrumbs\\Tests\\": "tests/"
    }
  },

  "conflict": {
    "davejamesmiller/laravel-breadcrumbs": "*",
    "diglactic/laravel-breadcrumbs": "*"
  },

  "replace": {
    "diglactic/laravel-breadcrumbs": "*"
  },

  "extra": {
    "laravel": {
      "providers": [
        "Diglactic\\Breadcrumbs\\ServiceProvider"
      ],
      "aliases": {
        "Breadcrumbs": "Diglactic\\Breadcrumbs\\Breadcrumbs"
      }
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
